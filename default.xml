<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default sync-j="4" revision="dunfell"/>

  <remote fetch="https://git.yoctoproject.org/git" name="yocto"/>
  <remote fetch="https://github.com/Freescale"     name="freescale"/>
  <remote fetch="https://git.openembedded.org"     name="oe"/>
  <remote fetch="https://github.com/meta-qt5"      name="QT5"/>
  <remote fetch="https://github.com/sbabic"        name="sbabic"/>

  <project remote="yocto"     upstream="dunfell"   name="poky"                    path="sources/poky"/>
  <project remote="yocto"     upstream="dunfell"   name="meta-raspberrypi"          path="sources/meta-raspberrypi"/>

  <project remote="oe"        upstream="dunfell"   name="meta-openembedded"       path="sources/meta-openembedded"/>

  <project remote="oe"        upstream="dunfell"   name="meta-python2"             path="sources/meta-python2"/>

  <project remote="freescale" upstream="dunfell"   name="Documentation"           path="sources/Documentation"/>

  <project remote="QT5"       upstream="dunfell"   name="meta-qt5"                path="sources/meta-qt5"/>
  <project remote="sbabic"    upstream="dunfell"   name="meta-swupdate"           path="sources/meta-swupdate"/>

</manifest>